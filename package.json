  "bugs": {
    "url": "https://github.com/tozielinski/next-brevo-api-helper/issues"
  },
  "homepage": "https://github.com/tozielinski/next-brevo-api-helper#readme",
  "devDependencies": {
    "@types/node": "^20.19.25",
    "typescript": "^5.0.0",
    "vitest": "^4.0.12"
  }
